    "release": "npm run build && npm publish dist",
    "coveralls": "nyc ava && nyc report --reporter=text-lcov | coveralls"
  },
  "bugs": {
    "url": "https://github.com/BlackDice/b3-chief/issues"
  },
  "author": {
    "name": "Daniel K.",
    "email": "fredy.c@seznam.cz"
  },
  "dependencies": {
