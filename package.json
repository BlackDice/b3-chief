    "babel-polyfill": "6.8.0",
    "common-tags": "1.2.1",
    "debounce": "1.0.0",
    "debug": "2.2.0",
    "events": "1.1.0",
    "invariant": "2.2.1",
    "lodash": "4.13.1",
    "purdy": "2.1.0",
    "stampit": "3.0.0-rc.3",
    "uid": "0.0.2",
    "warning": "github:blackdice/warning"
  },
  "devDependencies": {
    "ava": "0.14.0",
    "babel-core": "6.8.0",
    "babel-eslint": "6.0.4",
    "babel-plugin-dev-expression": "0.2.1",
    "babel-plugin-lodash": "3.1.4",
